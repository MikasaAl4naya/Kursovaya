@model WebApplication7.Models.Group_2

@{
    ViewBag.Title = "Список сданных работ";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2 class="mt-4">Выберите год поступления</h2>
@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    <div class="form-group mt-3">
        <div class="col-4">
            @Html.DropDownList("Year_of_receipt", null, htmlAttributes: new { @class = "form-control" })
        </div>
    </div>
    <div class="col-md-offset-2 col-4 mt-3">
        <input type="submit" value="Найти" class="btn btn-primary" />
    </div>
}


